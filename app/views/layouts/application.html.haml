    %link{:href => "/tyger/css/styleswitcher.css", :rel => "stylesheet"}
    %link{:href => "http://fonts.googleapis.com/css?family=Montserrat:400,700", :rel => "stylesheet", :type => "text/css"}
    %link{:href => "http://fonts.googleapis.com/css?family=Raleway:400,300,500,600,700,200", :rel => "stylesheet", :type => "text/css"}
    = stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true
    %script{:src => "/tyger/js/vendor/modernizr.js"}
    = csrf_meta_tags
  
  %body
    = yield
  
    = javascript_include_tag "application", "data-turbolinks-track" => true
    %script{:src => "/tyger/js/foundation.min.js"}
    %script{:src => "/tyger/js/foundation/foundation.accordion.js"}
    %script{:src => "/tyger/js/plugins.js"}
    %script{:src => "/tyger/js/jquery.cubeportfolio.min.js"}
    %script{:src => "/tyger/js/map.js"}
    %script{:src => "/tyger/js/custom.js"}
    / Start Style Switcher to remove
    %script{:src => "/tyger/js/styleswitcher.js"}
    %script{:src => "/tyger/js/jquery-cookie.js"}
    / End Style Switcher
    :javascript
      $(document).foundation();