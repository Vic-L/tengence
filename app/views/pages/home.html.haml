%section#contacts
  .row
    .medium-3.small-12.columns.animated{"data-animate" => "fadeInDown", "data-animate-delay" => "0"}
      .title-section
        %h3
          %strong Contact Us
    .medium-9.columns.animated{"data-animate" => "fadeInUp", "data-animate-delay" => "0"}
      .section-desc
        %h4
          %strong Find out how you can join our platform!
  .row
    #message
    = form_tag '/contact_us_email', remote: true, id: 'contactform' do
      .small-6.columns
        = text_field_tag "name", nil, placeholder: "Name"
      .small-6.columns
        = email_field_tag "email", nil, placeholder: "Email"
      .small-12.columns
        = text_area_tag "comments", nil, placeholder: "Message"
      .small-12.columns.text-center
        %button#submit{:type => "submit"} SEND
