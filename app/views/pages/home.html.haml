- content_for :javascript do
  :javascript
    Tengence.HomePage.initHeader();
    $('#sample-email-container').click(function(){
      $('#sample-email-modal').foundation('reveal', 'open');
    });
    $('#hide-all').click(function(){
      $(this).toggleClass('active');
      $('#show-all').toggleClass('active');
      $('.remainder-tender-source').slideUp();
    });
    $('#show-all').click(function(){
      $(this).toggleClass('active');
      $('#hide-all').toggleClass('active');
      $('.remainder-tender-source').slideDown();
    });
    $('#demo-email-form').validate({
      rules: {
        'demo_email': {
          required: true,
          email: true
        },
        'agree': {
          required: true
        }
      },
      messages: {
        'demo_email': {
          required: 'Please enter your email',
          email: 'Please enter a valid email'
        },
        'agree': {
          required: "Please acknowledge this checkbox"
        }
      }
    })
