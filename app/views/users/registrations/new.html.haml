            %li
              %a{:href => "#", :title => ""}
                %i.icon.tyger-twitter
            %li
              %a{:href => "#", :title => ""}
                %i.icon.tyger-facebook
            %li
              %a{:href => "#", :title => ""}
                %i.icon.tyger-google-plus
            %li
              %a{:href => "#", :title => ""}
                %i.icon.tyger-pinterest
        %p.white 2014 © Mordorthemes. All rights reserved.
  / end footer
/ end #wrapper
