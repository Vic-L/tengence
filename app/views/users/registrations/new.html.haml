      / end logo
    .medium-6.small-6.columns.text-right
      / start open/close menu
      %a#trigger-overlay{:href => "#", :title => ""}
        %i.icon.tyger-navicon
      / end open/close menu
/ end header
/ start navigation
.overlay-menu.overlay-hugeinc
  %a.overlay-close{:href => "#", :title => ""}
    %i.icon.tyger-close-empty
  %nav
    %ul
      %li
        %a.inner-link{:href => "#about"}
          %span About
      %li
        %a.inner-link{:href => "#services"}
          %span Services
      %li
        %a.inner-link{:href => "#team"}
          %span Team
      %li
