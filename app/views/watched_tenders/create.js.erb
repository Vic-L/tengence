// alert("#watch-tender-<%= @tender_ref_no %>");
$("#watch-tender-<%= @tender_ref_no %>").empty();
$("#watch-tender-<%= @tender_ref_no %>").append("<%= j link_to 'Unwatch', watched_tender_path(id: @tender_ref_no), method: 'delete', remote: true, class: 'button unwatch-button' %>");
$("#watch-tender-<%= @tender_ref_no %>").notify(
  "Successfully added to watchlist", "success", 
  { position: "top" }
);
$('html,body').removeClass('loading');